<div id="<%= dom_id municipe %>">
  <p>
    <strong>Name:</strong>
    <%= municipe.name %>
  </p>

  <p>
    <strong>Cpf:</strong>
    <%= municipe.cpf %>
  </p>

  <p>
    <strong>Cns:</strong>
    <%= municipe.cns %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= municipe.email %>
  </p>

  <p>
    <strong>Birthday:</strong>
    <%= municipe.birthday %>
  </p>

  <p>
    <strong>Phone:</strong>
    <%= municipe.phone %>
  </p>

  <p>
    <strong>Photo:</strong>
    <%= image_tag municipe.photo.variant(:thumb) if municipe.photo.attached? %>
  </p>

  <p>
    <strong>Status:</strong>
    <%= municipe.status %>
  </p>

</div>
